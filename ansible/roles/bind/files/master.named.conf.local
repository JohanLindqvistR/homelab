//include "/etc/bind/zones.rfc1918";

zone "draups.home" {
    type master;
    file "/etc/bind/zones/db.lan";
    allow-transfer { 192.168.1.234; };
    also-notify { 192.168.1.234; };
};

zone "draups.com" {
    type master;
    file "/etc/bind/zones/db.draups.com";
    allow-transfer { 192.168.1.234; };
    also-notify { 192.168.1.234; };
};