curl -L https://raw.githubusercontent.com/JohanLindqvistR/homelab/main/docker_stack/traefik/traefik-stack.yaml -o traefik-stack.yaml
docker stack deploy -c traefik-stack.yaml traefik

curl -L https://raw.githubusercontent.com/JohanLindqvistR/homelab/main/docker_stack/traefik/traefik.yml -o traefik.yml

curl -L https://raw.githubusercontent.com/JohanLindqvistR/homelab/main/docker_stack/traefik/config.yml -o config.yml
